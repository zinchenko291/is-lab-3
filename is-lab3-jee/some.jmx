        <JSR223Timer guiclass="TestBeanGUI" testclass="JSR223Timer" testname="Sync Updates">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="script">import java.util.concurrent.CyclicBarrier
import java.util.concurrent.TimeUnit

def key = &apos;barrier.syncUpdates&apos;
def barrier = props.get(key)
if (barrier == null) {
  barrier = new CyclicBarrier(2)
  props.put(key, barrier)
}
try {
  barrier.await(15, TimeUnit.SECONDS)
} catch (Exception e) {
  barrier.reset()
  log.warn(&apos;Sync Updates barrier skipped&apos;, e)
}</stringProp>

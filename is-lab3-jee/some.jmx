        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Generate Users" enabled="true">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="script">import java.security.KeyPairGenerator
import java.util.Base64

def admins = [&apos;admin1&apos;, &apos;admin2&apos;]
def fields = [&apos;pubkey&apos;, &apos;privkey&apos;, &apos;name&apos;, &apos;email&apos;]
props.put(&apos;admins.list&apos;, String.join(&apos;,&apos;, admins))

admins.each { prefix -&gt;
  fields.each { field -&gt;
    def val = vars.get(prefix + &apos;.&apos; + field)
    if (val != null &amp;&amp; !val.isBlank()) {
      props.put(prefix + &apos;.&apos; + field, val)
    } else {
      log.warn(&apos;Missing value for &apos; + prefix + &apos;.&apos; + field)
    }
  }
}

def gen = KeyPairGenerator.getInstance(&apos;Ed25519&apos;)
def now = System.currentTimeMillis()
def userPair = gen.generateKeyPair()
props.put(&apos;user.pubkey&apos;, Base64.encoder.encodeToString(userPair.public.encoded))
props.put(&apos;user.privkey&apos;, Base64.encoder.encodeToString(userPair.private.encoded))
props.put(&apos;user.name&apos;, &apos;user-&apos; + now)
props.put(&apos;user.email&apos;, &apos;user-&apos; + now + &apos;@example.com&apos;)</stringProp>

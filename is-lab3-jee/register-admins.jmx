        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Print Keys">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="script">def out = &quot;admin1.pubkey=&quot; + vars.get(&apos;admin1.pubkey&apos;) + &quot;\\n&quot; +
          &quot;admin1.privkey=&quot; + vars.get(&apos;admin1.privkey&apos;) + &quot;\\n&quot; +

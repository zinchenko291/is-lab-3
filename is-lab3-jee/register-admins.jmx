        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Generate Admin Keys">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="script">import java.security.KeyPairGenerator
import java.util.Base64

def gen = KeyPairGenerator.getInstance(&apos;Ed25519&apos;)
def admins = [
  [name: &apos;admin-one&apos;, email: &apos;admin-one@example.com&apos;],
  [name: &apos;admin-two&apos;, email: &apos;admin-two@example.com&apos;]
]

admins.eachWithIndex { admin, idx -&gt;
  def pair = gen.generateKeyPair()
  def key = &apos;admin&apos; + (idx + 1)
  vars.put(key + &apos;.pubkey&apos;, Base64.encoder.encodeToString(pair.public.encoded))
  vars.put(key + &apos;.privkey&apos;, Base64.encoder.encodeToString(pair.private.encoded))
  vars.put(key + &apos;.name&apos;, admin.name)
  vars.put(key + &apos;.email&apos;, admin.email)
}</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">false</stringProp>
        </JSR223Sampler>
        <hashTree/>
        <LoopController guiclass="LoopControlPanel" testclass="LoopController" testname="Register Loop">

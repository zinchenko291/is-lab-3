            <JSONPostProcessor guiclass="JSONPostProcessorGui" testclass="JSONPostProcessor" testname="Extract Challenge" enabled="true">
              <stringProp name="JSONPostProcessor.referenceNames">challenge</stringProp>
              <stringProp name="JSONPostProcessor.jsonPathExprs">$.challenge</stringProp>
              <stringProp name="JSONPostProcessor.match_numbers">1</stringProp>
            </JSONPostProcessor>
            <hashTree/>
          </hashTree>
          <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Sign Challenge">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="script">import java.security.KeyFactory
import java.security.Signature
import java.security.spec.PKCS8EncodedKeySpec
import java.util.Base64

def privKeyB64 = vars.get(&apos;admin.privkey&apos;)
def challenge = vars.get(&apos;challenge&apos;)
def privBytes = Base64.decoder.decode(privKeyB64)
def keySpec = new PKCS8EncodedKeySpec(privBytes)
def keyFactory = KeyFactory.getInstance(&apos;Ed25519&apos;)
def privateKey = keyFactory.generatePrivate(keySpec)
def signature = Signature.getInstance(&apos;Ed25519&apos;)
signature.initSign(privateKey)
signature.update(Base64.decoder.decode(challenge))
vars.put(&apos;signature&apos;, Base64.encoder.encodeToString(signature.sign()))</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">false</stringProp>
          </JSR223Sampler>
          <hashTree/>

        <hashTree>
          <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Select Admin">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="script">def idx = (vars.get(&apos;admin.idx&apos;) ?: &apos;1&apos;) as int
def key = &apos;admin&apos; + idx
vars.put(&apos;admin.name&apos;, vars.get(key + &apos;.name&apos;))
vars.put(&apos;admin.email&apos;, vars.get(key + &apos;.email&apos;))
vars.put(&apos;admin.pubkey&apos;, vars.get(key + &apos;.pubkey&apos;))
vars.put(&apos;admin.privkey&apos;, vars.get(key + &apos;.privkey&apos;))
vars.put(&apos;admin.idx&apos;, (idx == 1 ? &apos;2&apos; : &apos;1&apos;))</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">false</stringProp>
          </JSR223Sampler>

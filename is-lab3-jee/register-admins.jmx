                  <stringProp name="Argument.value">{&quot;pubkey&quot;:&quot;${admin.pubkey}&quot;,&quot;signature&quot;:&quot;${signature}&quot;}</stringProp>
                  <stringProp name="Argument.metadata">=</stringProp>
